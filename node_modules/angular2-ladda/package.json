{
  "name": "angular2-ladda",
  "version": "1.2.3",
  "description": "Angular Ladda module",
  "files": [
    "module/*.d.ts",
    "module/*.js",
    "module/*.js.map",
    "module/*.json"
  ],
  "main": "./module/module.js",
  "types": "./module/module.d.ts",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "prepublish": "ngc -p ./tsconfig.publish.json"
  },
  "keywords": [
    "angular",
    "angular2",
    "ng2",
    "ladda",
    "module",
    "package",
    "typescript"
  ],
  "author": {
    "name": "Paul Moff"
  },
  "contributors": [
    {
      "name": "Theodore Brown",
      "url": "https://github.com/theodorejb"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moff/angular2-ladda.git"
  },
  "license": "MIT",
  "dependencies": {
    "ladda": "^1.0.5"
  },
  "peerDependencies": {
    "@angular/core": "^4.0.0"
  },
  "devDependencies": {
    "@angular/common": "^4.4.3",
    "@angular/compiler": "^4.4.3",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/forms": "^4.4.3",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "@angular/platform-server": "^4.4.3",
    "@angular/router": "^4.4.3",
    "@types/ladda": "^0.9.30",
    "concurrently": "^3.5.0",
    "core-js": "^2.5.1",
    "lite-server": "^2.3.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "5.4.3",
    "systemjs": "^0.20.19",
    "typescript": "2.3.2",
    "zone.js": "^0.8.17"
  }
}
